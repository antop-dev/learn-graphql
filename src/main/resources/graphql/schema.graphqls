type Region {
    id: ID!
    name: String!
}

type Country {
    id: ID!
    name: String!
    region: Region!
}

type Query {
    getRegionById(id: ID): Region
    getRegions: [Region]
    getRegionsByName(name: String): [Region]

    getCountryById(id: ID): Country
    getCountries: [Country]
    getCountriesByName(name: String): [Country]
}
